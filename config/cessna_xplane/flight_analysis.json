{
	"data_handling": {
		"target": "flight_analysis",
		"period": 200
	},
	"maneuver_planner": {
		"period": 10,
		"maneuver_sets": {
			"roll_maneuver": {
				"maneuvers": [
					{
						"overrides": {
							"pid/roll": 0,
							"pid/climb_angle": 0
						},
						"transition": {
							"steady_state": {
								"duration": 2000,
								"sensor_values": {
									"attitude_x": 0.1,
									"attitude_y": 0.1,
									"air_speed": 1
								}
							}
						}
					},
					{
						"overrides": {
						},
						"maintains": [
							"pid/pitch"
						],
						"waveforms": {
							"pid/roll": {
								"sine": {
									"amplitude": 30,
									"frequency": 0.25
								}
							}
						},
						"transition": {
							"duration": {
								"duration": 10000
							}
						}
					}
				],
				"save_as": "roll_maneuver.txt"
			},
			"get_trim": {
				"maneuvers": [
					{
						"overrides": {
							"pid/roll": 0.0,
							"pid/pitch": 0.0
						},
						"transition": {
							"steady_state": {
								"duration": 5000,
								"sensor_values": {
									"attitude_x": 0.01,
									"attitude_y": 0.01
								}
							}
						}
					}
				]
			},
