syms psi_1 theta phi;
syms u v w;

Rz = [cos(psi_1), -sin(psi_1), 0; sin(psi_1), cos(psi_1), 0; 0, 0, 1];
Ry = [cos(theta), 0, sin(theta); 0, 1, 0; -sin(theta), 0, cos(theta)];
Rx = [1, 0, 0; 0, cos(phi), -sin(phi); 0, sin(phi), cos(phi)];
vel = [u; v; w];

xyz_dot = Rz * Ry * Rx * vel;

vx = xyz_dot(1);
vy = xyz_dot(2);
vz = -xyz_dot(3);

x_u = diff(vx, u);
x_v = diff(vx, v);
x_w = diff(vx, w);

y_u = diff(vy, u);
y_v = diff(vy, v);
y_w = diff(vy, w);

z_u = diff(vz, u);
z_v = diff(vz, v);
z_w = diff(vz, w);

x_phi = diff(vx, phi);
x_theta = diff(vx, theta);
x_psi = diff(vx, psi_1);

y_phi = diff(vy, phi);
y_theta = diff(vy, theta);
y_psi = diff(vy, psi);

z_phi = diff(vz, phi);
z_theta = diff(vz, theta);
z_psi = diff(vz, psi);